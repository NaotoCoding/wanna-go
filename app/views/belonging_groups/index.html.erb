<table class="table">
  <thead>
    <tr>
      <th>グループ名</th>
      <th>オーナー</th>
      <th>人数</th>
    </tr>
  </thead>
  <tbody>
    <% @belonging_groups.each do |belonging_group| %>
      <tr>
        <td><%= belonging_group.name %></td>
        <td><%= belonging_group.owner.name %></td>
        <td><%= belonging_group.group_users.count %></td>
      </tr>
    <% end %>
  </tbody>
</table>
